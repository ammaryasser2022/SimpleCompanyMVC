@model ResetPasswordViewModel
@{
    ViewData["Title"] = "ResetPassword";
    Layout = "~/Views/Shared/AuthLayout.cshtml";
}


<div class="main-agileinfo">
	<div asp-validation-summary="All" class=" alert alert-danger"> </div>

	<div class="agileits-top">
		<form asp-action="ResetPassword" method="post">

			<br />
			<label asp-for="Password" class="col-form-label"> </label>
			<input class="text" asp-for="Password" placeholder="Password">
			<br />
			<label asp-for="ConfirmPassword" class="col-form-label"> </label>
			<input class="text w3lpass" asp-for="ConfirmPassword" placeholder="Confirm Password">
			<br />
		
			<input type="submit" value="RESET">
		</form>
	</div>
</div>



